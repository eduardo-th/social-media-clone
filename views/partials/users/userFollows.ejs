<div class="row justify-content-evenly">
    <% for(let follow of foundUser.follows) {%> 
      <div class="col-md-2 col-3 px-1 mx-1 text-center">        
        <img class=" img-fluid" src="<%= follow.avatar.thumbnail %> " alt="">
        <a href="/users/<%= follow.id %> "><span><%= follow.username %> </span></a>
      </div>
      <% } %> 
  </div>